{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location":{
        "type": "string",
        "allowedValues": [
          "Central US",
          "East US",
          "East US 2",
          "North Central US",
          "South Central US",
          "West US"]
    }
},
"variables": {
    "baseUrl": "https://raw.githubusercontent.com/komalio/komali-test/aseenv/aseenv/",
    "vnetSetup": "[concat(variables('baseUrl'),'asevnet.json')]",
    "aseRouteSetup": "[concat(variables('baseUrl'),'aseroute.json')]",
    "aseNsgSetup": "[concat(variables('baseUrl'),'asensg.json')]",
    "aseSetup": "[concat(variables('baseUrl'),'asehostenv.json')]",
    "dnsSuffix": "[concat('appdsfd',substring(uniqueString(resourceGroup().id),4,9))]",
    "internalLoadBalancingMode": "3",
    "vnetName": "asevnet",
    "subnetName": "[concat('asesubnet',substring(uniqueString(resourceGroup().id),4,9))]",
    "subnetNSGName": "asensg",
    "subnetRouteTableName": "aseroute",
    "appName": "[concat('aseapp',substring(uniqueString(resourceGroup().id),4,9))]",
    "vnetAddress": "10.0.0.0/16",
    "subnetAddress": "10.0.1.0/24",
    "kind": "ASEV2",
    "subnetRouteId": "[resourceId('Microsoft.Network/routeTables', variables('subnetRouteTableName'))]",
    "subnetNSGId": "[resourceId('Microsoft.Network/networkSecurityGroups', variables('subnetNSGName'))]",
    "vnetId": "[resourceId('Microsoft.Network/virtualNetworks/',variables('vnetName'))]",
    "aseplan": "[concat(variables('baseUrl'),'aseserverplan.json')]"
},
"resources": [
    {
        "name": "aseRouteSetup",
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2017-08-01",
        "properties": {
        "mode": "Incremental",
        "templateLink": {
        "uri": "[variables('aseRouteSetup')]",
        "contentVersion": "1.0.0.0"
        },                                  
        "parameters": {   
            "location": {           
               "value": "[parameters('location')]"              
          },
            "subnetRouteTableName": {         
                "value": "[variables('subnetRouteTableName')]"        
          }
        }             
      }
    },
    {
        "name": "aseNsgSetup",
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2017-08-01",
        "properties": {
        "mode": "Incremental",
        "templateLink": {
        "uri": "[variables('aseNsgSetup')]",
        "contentVersion": "1.0.0.0"
        },                                  
        "parameters": {   
            "location": {           
               "value": "[parameters('location')]"              
          },
            "subnetNSGName": {         
                "value": "[variables('subnetNSGName')]"        
          }
        }             
      }
    },
    {
        "name": "vnetSetup",
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2017-08-01",
        "dependsOn": [
            "aseRouteSetup",
            "aseNsgSetup"
          ],
        "properties": {
        "mode": "Incremental",
        "templateLink": {
        "uri": "[variables('vnetSetup')]",
        "contentVersion": "1.0.0.0"
        },                                  
        "parameters": {   
            "location": {           
               "value": "[parameters('location')]"              
          },
            "vnetName": {         
                "value": "[variables('vnetName')]"        
          },
            "subnetName": {           
            "value": "[variables('subnetName')]"              
         },
            "vnetAddress": {           
            "value": "[variables('vnetAddress')]"              
         },
             "subnetAddress": {           
            "value": "[variables('subnetAddress')]"              
         },
         "subnetNSGId": {           
            "value": "[variables('subnetNSGId')]"              
       },
          "subnetRouteId": {           
            "value": "[variables('subnetRouteId')]"              
       }
        }             
      }
    },
     {
          "name": "aseSetup",
          "type": "Microsoft.Resources/deployments",
          "apiVersion": "2017-08-01",
          "dependsOn": [
            "vnetSetup"
          ],
          "properties": {
          "mode": "Incremental",
          "templateLink": {
          "uri": "[variables('aseSetup')]",
          "contentVersion": "1.0.0.0"
            },                                  
            "parameters": {   
                "location": {           
                   "value": "[parameters('location')]"              
              },
                "appName": {         
                    "value": "[variables('appName')]"        
              },
                 "kind": {           
                    "value": "[variables('kind')]"              
              },
                 "vnetId": {           
                  "value": "[variables('vnetId')]"              
              },
                 "subnetName": {           
                   "value": "[variables('subnetName')]"              
              },
                "dnsSuffix": {           
                    "value": "[variables('dnsSuffix')]"              
              },  
                 "internalLoadBalancingMode": {           
                    "value": "[variables('internalLoadBalancingMode')]"              
              }
            }             
          }
        },
        {
          "name": "aseplan",
          "type": "Microsoft.Resources/deployments",
          "apiVersion": "2017-08-01",
          "dependsOn": [
            "aseSetup"
          ],
          "properties": {
          "mode": "Incremental",
          "templateLink": {
          "uri": "[variables('aseplan')]",
          "contentVersion": "1.0.0.0"
          },                                  
          "parameters": {   
            "location": {           
              "value": "[parameters('location')]"              
         },
         "appName": {         
          "value": "[variables('appName')]"        
    }
          }             
        }
      }
      ]
  
  }
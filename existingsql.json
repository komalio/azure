{
    "$schema": "http://schema.management.azure.com/schemas/2014-04-01-preview/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "azureSQL": {
            "type": "object"
        },
        "apiVersions": {
            "type": "object"
        }
    },
    "variables": {},
    "resources": [
        {
            "apiVersion": "[parameters('apiVersions').sqlDBApiVersion]",
            "type": "Microsoft.Sql/servers/databases",
            "location": "[parameters('azureSQL').location]",
            "name": "[concat(parameters('azureSQL').serverName,'/',parameters('azureSQL').databaseName)]",
            "properties": {
                "collation": "[parameters('azureSQL').collation]",
                "edition": "[parameters('azureSQL').edition]",
                "maxSizeBytes": "[parameters('azureSQL').maxSizeBytes]",
                "requestedServiceObjectiveName": "[parameters('azureSQL').requestedServiceObjectiveName]"
            },
            "dependsOn": [
                "/subscriptions/2927c217-b119-4d3b-8a13-82a1c3a16c8f/resourceGroups/IOT-DemoLabs-Do-Not-Delete/providers/Microsoft.Sql/servers/emsdemosqlsrv"
                
            ],
            "resources": [
                {
                    "name": "myImport1",
                    "type": "extensions",
                    "apiVersion": "[parameters('apiVersions').sqlDBApiVersion]",
                    "dependsOn": [
                        "/subscriptions/2927c217-b119-4d3b-8a13-82a1c3a16c8f/resourceGroups/IOT-DemoLabs-Do-Not-Delete/providers/Microsoft.Sql/servers/emsdemosqlsrv/databases/iotdb"
                    ],
                    "properties": {
                        "storageKeyType": "[parameters('azureSQL').storageKeyType]",
                        "storageKey": "[parameters('azureSQL').storageKey]",
                        "storageUri": "[parameters('azureSQL').storageUri]",
                        "administratorLogin": "[parameters('azureSQL').sqlAuthenticationLogin]",
                        "administratorLoginPassword": "[parameters('azureSQL').sqlAuthenticationPassword]",
                        "operationMode": "Import"
                    }
                }
            ]
        },
        {
            "apiVersion": "[parameters('apiVersions').sqlDBApiVersion]",
            "type": "Microsoft.Sql/servers/databases",
            "location": "[parameters('azureSQL').location]",
            "name": "[concat(parameters('azureSQL').serverName,'/',parameters('azureSQL').databaseName1)]",
            "properties": {
                "collation": "[parameters('azureSQL').collation]",
                "edition": "[parameters('azureSQL').edition]",
                "maxSizeBytes": "[parameters('azureSQL').maxSizeBytes]",
                "requestedServiceObjectiveName": "[parameters('azureSQL').requestedServiceObjectiveName]"
            },
            "dependsOn": [
                "/subscriptions/2927c217-b119-4d3b-8a13-82a1c3a16c8f/resourceGroups/IOT-DemoLabs-Do-Not-Delete/providers/Microsoft.Sql/servers/emsdemosqlsrv"
            ],
            "resources": [
                {
                    "name": "myImport2",
                    "type": "extensions",
                    "apiVersion": "[parameters('apiVersions').sqlDBApiVersion]",
                    "dependsOn": [
                        "/subscriptions/2927c217-b119-4d3b-8a13-82a1c3a16c8f/resourceGroups/IOT-DemoLabs-Do-Not-Delete/providers/Microsoft.Sql/servers/emsdemosqlsrv/databases/webdb"
                    ],
                    "properties": {
                        "storageKeyType": "[parameters('azureSQL').storageKeyType]",
                        "storageKey": "[parameters('azureSQL').storageKey]",
                        "storageUri": "[parameters('azureSQL').storageUri1]",
                        "administratorLogin": "[parameters('azureSQL').sqlAuthenticationLogin]",
                        "administratorLoginPassword": "[parameters('azureSQL').sqlAuthenticationPassword]",
                        "operationMode": "Import"
                    }
                }
            ]
        }
    ],
    "outputs": {
        "iotDbName": {
            "type": "string",
            "value": "[parameters('azureSQL').databaseName]"
        }
    }
}
